{
  "name": "tripwase-cli",
  "version": "1.0.0",
  "description": "TripWase with CLI Templates functionality",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "templates": "node scripts/activity-manager.js",
    "planner": "node scripts/trip-planner.js",
    "stats": "node scripts/activity-manager.js stats",
    "list-trips": "node scripts/trip-planner.js list",
    "backend": "cd backend && npm run dev",
    "dev": "concurrently \"npm start\" \"npm run backend\"",
    "dev:full": "concurrently --names \"FRONTEND,BACKEND\" --prefix-colors \"cyan,green\" \"npm start\" \"cd backend && npm run dev\"",
    "setup:backend": "cd backend && npm install",
    "db:setup": "cd backend && npm run db:setup",
    "db:studio": "cd backend && npm run db:studio"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tanstack/react-query": "^5.87.4",
    "@types/leaflet": "^1.9.20",
    "cors": "^2.8.5",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "react": "^18.2.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^5.1.0"
  },
  "keywords": [
    "travel",
    "planning",
    "cli",
    "templates",
    "react"
  ],
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__template": "^7.4.4",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "jest": "^27.5.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
